# Default config for display profiles
# Find display with 'swaymsg -t get_outputs'

profile laptop {
  output eDP-1 enable mode 2256x1504@59.999Hz adaptive_sync on scale 1.25
  exec swaymsg workspace 1, move workspace to eDP-1
}

profile ts4dock {
  output eDP-1 disable
  output DP-8 enable mode 3840x1600@120.045Hz adaptive_sync on position 0,1440 scale 1
  output DP-7 enable mode 3440x1440@59.973Hz adaptive_sync on position 190,0 transform 180 scale 1
  exec swaymsg workspace 1, move workspace to DP-8
  exec swaymsg workspace 2, move workspace to DP-7
}
